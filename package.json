    "lodash": "^4.17.21",
    "lucide-react": "^0.539.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.0",
    "recharts": "^3.1.2"
