    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.7.0",
    "@mui/x-date-pickers": "^8.7.0",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "browser-image-compression": "^2.0.2",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.12",
    "jspdf": "^3.0.1",
